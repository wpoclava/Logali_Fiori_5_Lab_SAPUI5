<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="logaligroup.Customers.controller.Detail"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>detailPageTitle}" showNavButton="true" navButtonPress=".onNavBack">
		<ObjectHeader title="{customerWz>ContactName}">
			<attributes>
				<ObjectAttribute text="{customerWz>Address}"/>
				<ObjectAttribute text="{customerWz>City}"/>
				<ObjectAttribute text="{customerWz>PostalCode}"/>
				<ObjectAttribute text="{customerWz>Country}"/>
				<ObjectAttribute text="{customerWz>Phone}"/>
			</attributes>
		</ObjectHeader>
	</Page>
</mvc:View>